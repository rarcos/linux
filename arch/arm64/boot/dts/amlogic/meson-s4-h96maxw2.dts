/dts-v1/;

/ {
	compatible = "h96maxw2", "amlogic,ap201", "amlogic,s4";
	interrupt-parent = <0x01>;
	#address-cells = <0x02>;
	#size-cells = <0x02>;
	model = "H96 Max W2";

	cpus {
		#address-cells = <0x02>;
		#size-cells = <0x00>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a35";
			reg = <0x00 0x00>;
			enable-method = "psci";
			phandle = <0x26>;
		};

		cpu@1 {
			device_type = "cpu";
			compatible = "arm,cortex-a35";
			reg = <0x00 0x01>;
			enable-method = "psci";
		};

		cpu@2 {
			device_type = "cpu";
			compatible = "arm,cortex-a35";
			reg = <0x00 0x02>;
			enable-method = "psci";
		};

		cpu@3 {
			device_type = "cpu";
			compatible = "arm,cortex-a35";
			reg = <0x00 0x03>;
			enable-method = "psci";
		};
	};

	gpu_opp_table {
		compatible = "operating-points-v2";
		phandle = <0x1e>;

		opp-285 {
			opp-hz = <0x00 0x1107a769>;
			opp-microvolt = <0x47e>;
		};

		opp-400 {
			opp-hz = <0x00 0x17d78400>;
			opp-microvolt = <0x47e>;
		};

		opp-500 {
			opp-hz = <0x00 0x1dcd6500>;
			opp-microvolt = <0x47e>;
		};

		opp-666 {
			opp-hz = <0x00 0x27bc86aa>;
			opp-microvolt = <0x47e>;
		};

		opp-846 {
			opp-hz = <0x00 0x326cef80>;
			opp-microvolt = <0x47e>;
		};
	};

	audio-controller-0 {
		compatible = "amlogic,axg-tdm-iface";
		#sound-dai-cells = <0x00>;
		sound-name-prefix = "TDM_A";
		clocks = <0x02 0x31 0x02 0x4f 0x02 0x56>;
		clock-names = "mclk", "sclk", "lrclk";
		assigned-clocks = <0x02 0x3b>;
		assigned-clock-parents = <0x03 0x11>;
	};

	audio-controller-1 {
		compatible = "amlogic,axg-tdm-iface";
		#sound-dai-cells = <0x00>;
		sound-name-prefix = "TDM_B";
		clocks = <0x02 0x32 0x02 0x50 0x02 0x57>;
		clock-names = "mclk", "sclk", "lrclk";
		assigned-clocks = <0x02 0x3c>;
		assigned-clock-parents = <0x03 0x11>;
	};

	audio-controller-2 {
		compatible = "amlogic,axg-tdm-iface";
		#sound-dai-cells = <0x00>;
		sound-name-prefix = "TDM_C";
		clocks = <0x02 0x33 0x02 0x51 0x02 0x58>;
		clock-names = "mclk", "sclk", "lrclk";
		assigned-clocks = <0x02 0x3d>;
		assigned-clock-parents = <0x03 0x11>;
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts = <0x01 0x0d 0xf08 0x01 0x0e 0xf08 0x01 0x0b 0xf08 0x01 0x0a 0xf08>;
	};

	psci {
		compatible = "arm,psci-1.0";
		method = "smc";
	};

	xtal-clk {
		compatible = "fixed-clock";
		clock-frequency = <0x16e3600>;
		clock-output-names = "xtal";
		#clock-cells = <0x00>;
		phandle = <0x0a>;
	};

	firmware {

		secure-monitor {
			compatible = "amlogic,meson-gxbb-sm";

			power-controller {
				compatible = "amlogic,meson-s4-pwrc";
				#power-domain-cells = <0x01>;
				phandle = <0x10>;
			};
		};
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		ranges;

		interrupt-controller@fff01000 {
			compatible = "arm,gic-400";
			#interrupt-cells = <0x03>;
			#address-cells = <0x00>;
			interrupt-controller;
			reg = <0x00 0xfff01000 0x00 0x1000 0x00 0xfff02000 0x00 0x2000 0x00 0xfff04000 0x00 0x2000 0x00 0xfff06000 0x00 0x2000>;
			interrupts = <0x01 0x09 0xf04>;
			phandle = <0x01>;
		};

		bus@fe000000 {
			compatible = "simple-bus";
			reg = <0x00 0xfe000000 0x00 0x480000>;
			#address-cells = <0x02>;
			#size-cells = <0x02>;
			ranges = <0x00 0x00 0x00 0xfe000000 0x00 0x480000>;

			bus@36000 {
				compatible = "simple-bus";
				#address-cells = <0x02>;
				#size-cells = <0x02>;
				ranges = <0x00 0x00 0x00 0x36000 0x00 0x2000>;

				video-lut@48 {
					compatible = "amlogic,canvas";
					reg = <0x00 0x48 0x00 0x14>;
					phandle = <0x1f>;
				};
			};

			hdmi-tx@300000 {
				compatible = "amlogic,meson-s4-dw-hdmi";
				reg = <0x00 0x300000 0x00 0x10000>;
				interrupts = <0x00 0xcc 0x01>;
				resets = <0x04 0x10 0x04 0x16 0x04 0x1d>;
				reset-names = "hdmitx_apb", "hdmitx", "hdmitx_phy";
				clocks = <0x05 0x43 0x05 0xcb 0x05 0xd2>;
				clock-names = "isfr", "iahb", "venci";
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				#sound-dai-cells = <0x00>;
				status = "okay";
				pinctrl-0 = <0x06 0x07>;
				pinctrl-names = "default";

				port@0 {
					reg = <0x00>;

					endpoint {
						remote-endpoint = <0x08>;
						phandle = <0x21>;
					};
				};

				port@1 {
					reg = <0x01>;

					endpoint {
						remote-endpoint = <0x09>;
						phandle = <0x2c>;
					};
				};
			};

			clock-controller@0 {
				compatible = "amlogic,s4-peripherals-clkc";
				reg = <0x00 0x00 0x00 0x49c>;
				clocks = <0x03 0x03 0x03 0x0d 0x03 0x05 0x03 0x07 0x03 0x09 0x03 0x0b 0x03 0x11 0x03 0x0f 0x03 0x19 0x03 0x1b 0x03 0x1d 0x03 0x1f 0x03 0x14 0x0a>;
				clock-names = "fclk_div2", "fclk_div2p5", "fclk_div3", "fclk_div4", "fclk_div5", "fclk_div7", "hifi_pll", "gp0_pll", "mpll0", "mpll1", "mpll2", "mpll3", "hdmi_pll", "xtal";
				#clock-cells = <0x01>;
				phandle = <0x05>;
			};

			clock-controller@8000 {
				compatible = "amlogic,s4-pll-clkc";
				reg = <0x00 0x8000 0x00 0x1e8>;
				clocks = <0x0a>;
				clock-names = "xtal";
				#clock-cells = <0x01>;
				phandle = <0x03>;
			};

			watchdog@2100 {
				compatible = "amlogic,s4-wdt", "amlogic,t7-wdt";
				reg = <0x00 0x2100 0x00 0x10>;
				clocks = <0x0a>;
			};

			pinctrl@4000 {
				compatible = "amlogic,meson-s4-periphs-pinctrl";
				#address-cells = <0x02>;
				#size-cells = <0x02>;
				ranges;
				phandle = <0x0b>;

				bank@4000 {
					reg = <0x00 0x4000 0x00 0x4c 0x00 0x40c0 0x00 0x220>;
					reg-names = "mux", "gpio";
					gpio-controller;
					#gpio-cells = <0x02>;
					gpio-ranges = <0x0b 0x00 0x00 0x52>;
					phandle = <0x1a>;
				};

				remote-pin {

					mux {
						groups = "remote_in";
						function = "remote_in";
						bias-disable;
					};
				};

				i2c0-pins1 {

					mux {
						groups = "i2c0_sda", "i2c0_scl";
						function = "i2c0";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c1-pins1 {

					mux {
						groups = "i2c1_sda_c", "i2c1_scl_c";
						function = "i2c1";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c1-pins2 {

					mux {
						groups = "i2c1_sda_d", "i2c1_scl_d";
						function = "i2c1";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c1-pins3 {

					mux {
						groups = "i2c1_sda_h", "i2c1_scl_h";
						function = "i2c1";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c1-pins4 {

					mux {
						groups = "i2c1_sda_x", "i2c1_scl_x";
						function = "i2c1";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c2-pins1 {

					mux {
						groups = "i2c2_sda_d", "i2c2_scl_d";
						function = "i2c2";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c2-pins2 {

					mux {
						groups = "i2c2_sda_h8", "i2c2_scl_h9";
						function = "i2c2";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c2-pins3 {

					mux {
						groups = "i2c2_sda_h0", "i2c2_scl_h1";
						function = "i2c2";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c3-pins1 {

					mux {
						groups = "i2c3_sda_x", "i2c3_scl_x";
						function = "i2c3";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c3-pins2 {

					mux {
						groups = "i2c3_sda_z", "i2c3_scl_z";
						function = "i2c3";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c4-pins1 {

					mux {
						groups = "i2c4_sda_c", "i2c4_scl_c";
						function = "i2c4";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c4-pins2 {

					mux {
						groups = "i2c4_sda_d", "i2c4_scl_d";
						function = "i2c4";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c4-pins3 {

					mux {
						groups = "i2c4_sda_z", "i2c4_scl_z";
						function = "i2c4";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				nand-pins {

					mux {
						groups = "emmc_nand_d0", "emmc_nand_d1", "emmc_nand_d2", "emmc_nand_d3", "emmc_nand_d4", "emmc_nand_d5", "emmc_nand_d6", "emmc_nand_d7", "nand_ce0", "nand_ale", "nand_cle", "nand_wen_clk", "nand_ren_wr";
						function = "nand";
						input-enable;
					};
				};

				pwm-a-pins1 {

					mux {
						groups = "pwm_a_d";
						function = "pwm_a";
					};
				};

				pwm-a-pins2 {

					mux {
						groups = "pwm_a_x";
						function = "pwm_a";
					};
				};

				pwm-b-pins1 {

					mux {
						groups = "pwm_b_d";
						function = "pwm_b";
					};
				};

				pwm-b-pins2 {

					mux {
						groups = "pwm_b_x";
						function = "pwm_b";
					};
				};

				pwm-c-pins1 {

					mux {
						groups = "pwm_c_d";
						function = "pwm_c";
					};
				};

				pwm-c-pins2 {

					mux {
						groups = "pwm_c_x";
						function = "pwm_c";
					};
				};

				pwm-d-pins1 {

					mux {
						groups = "pwm_d_d";
						function = "pwm_d";
					};
				};

				pwm-d-pins2 {

					mux {
						groups = "pwm_d_h";
						function = "pwm_d";
					};
				};

				pwm-e-pins1 {
					phandle = <0x0e>;

					mux {
						groups = "pwm_e_x";
						function = "pwm_e";
					};
				};

				pwm-e-pins2 {

					mux {
						groups = "pwm_e_z";
						function = "pwm_e";
					};
				};

				pwm-f-pins1 {

					mux {
						groups = "pwm_f_x";
						function = "pwm_f";
					};
				};

				pwm-f-pins2 {

					mux {
						groups = "pwm_f_z";
						function = "pwm_f";
					};
				};

				pwm-g-pins1 {

					mux {
						groups = "pwm_g_d";
						function = "pwm_g";
					};
				};

				pwm-g-pins2 {

					mux {
						groups = "pwm_g_z";
						function = "pwm_g";
					};
				};

				pwm-h-pins {

					mux {
						groups = "pwm_h";
						function = "pwm_h";
					};
				};

				pwm-i-pins1 {

					mux {
						groups = "pwm_i_d";
						function = "pwm_i";
					};
				};

				pwm-i-pins2 {

					mux {
						groups = "pwm_i_h";
						function = "pwm_i";
					};
				};

				pwm-j-pins {

					mux {
						groups = "pwm_j";
						function = "pwm_j";
					};
				};

				pwm-a-hiz-pins {

					mux {
						groups = "pwm_a_hiz";
						function = "pwm_a_hiz";
					};
				};

				pwm-b-hiz-pins {

					mux {
						groups = "pwm_b_hiz";
						function = "pwm_b_hiz";
					};
				};

				pwm-c-hiz-pins {

					mux {
						groups = "pwm_c_hiz";
						function = "pwm_c_hiz";
					};
				};

				pwm-g-hiz-pins {

					mux {
						groups = "pwm_g_hiz";
						function = "pwm_g_hiz";
					};
				};

				sdcard-pins {
					phandle = <0x18>;

					mux {
						groups = "sdcard_d0_c", "sdcard_d1_c", "sdcard_d2_c", "sdcard_d3_c", "sdcard_clk_c", "sdcard_cmd_c";
						function = "sdcard";
						bias-pull-up;
						drive-strength-microamp = <0xfa0>;
					};
				};

				sdcard-clk-gate-pins {
					phandle = <0x19>;

					mux {
						groups = "GPIOC_4";
						function = "gpio_periphs";
						bias-pull-down;
						drive-strength-microamp = <0xfa0>;
					};
				};

				emmc-pins {
					phandle = <0x1b>;

					mux-0 {
						groups = "emmc_nand_d0", "emmc_nand_d1", "emmc_nand_d2", "emmc_nand_d3", "emmc_nand_d4", "emmc_nand_d5", "emmc_nand_d6", "emmc_nand_d7", "emmc_cmd";
						function = "emmc";
						bias-pull-up;
						drive-strength-microamp = <0xfa0>;
					};

					mux-1 {
						groups = "emmc_clk";
						function = "emmc";
						bias-pull-up;
						drive-strength-microamp = <0xfa0>;
					};
				};

				emmc-ds-pins {
					phandle = <0x1c>;

					mux {
						groups = "emmc_nand_ds";
						function = "emmc";
						bias-pull-down;
						drive-strength-microamp = <0xfa0>;
					};
				};

				emmc-clk-gate-pins {
					phandle = <0x1d>;

					mux {
						groups = "GPIOB_8";
						function = "gpio_periphs";
						bias-pull-down;
						drive-strength-microamp = <0xfa0>;
					};
				};

				sdio-pins {
					phandle = <0x14>;

					mux {
						groups = "sdio_d0", "sdio_d1", "sdio_d2", "sdio_d3", "sdio_clk", "sdio_cmd";
						function = "sdio";
						bias-pull-up;
						drive-strength-microamp = <0xfa0>;
					};
				};

				sdio-clk-gate-pins {
					phandle = <0x15>;

					mux {
						groups = "GPIOX_4";
						function = "gpio_periphs";
						bias-pull-down;
						drive-strength-microamp = <0xfa0>;
					};
				};

				spicc0-pins_x {

					mux {
						groups = "spi_a_mosi_x", "spi_a_miso_x", "spi_a_clk_x";
						function = "spi_a";
						drive-strength-microamp = <0xbb8>;
					};
				};

				spicc0-pins-h {

					mux {
						groups = "spi_a_mosi_h", "spi_a_miso_h", "spi_a_clk_h";
						function = "spi_a";
						drive-strength-microamp = <0xbb8>;
					};
				};

				spicc0-pins-z {

					mux {
						groups = "spi_a_mosi_z", "spi_a_miso_z", "spi_a_clk_z";
						function = "spi_a";
						drive-strength-microamp = <0xbb8>;
					};
				};

				hdmitx-hpd {
					phandle = <0x06>;

					mux {
						groups = "hdmitx_hpd_in";
						function = "hdmitx";
						bias-disable;
					};
				};

				hdmitx-ddc {
					phandle = <0x07>;

					mux {
						groups = "hdmitx_sda", "hdmitx_sck";
						function = "hdmitx";
						bias-disable;
						drive-strength-microamp = <0xfa0>;
					};
				};

				a_uart {

					mux {
						groups = "uart_a_tx", "uart_a_rx", "uart_a_cts", "uart_a_rts";
						function = "uart_a";
					};
				};

				c_uart {

					mux {
						groups = "uart_c_tx", "uart_c_rx";
						function = "uart_c";
					};
				};

				d_uart1 {

					mux {
						groups = "uart_d_tx_h", "uart_d_rx_h";
						function = "uart_d";
					};
				};

				d_uart2 {

					mux {
						groups = "uart_d_tx_z", "uart_d_rx_z";
						function = "uart_d";
					};
				};

				e_uart1 {

					mux {
						groups = "uart_e_tx_h", "uart_e_rx_h";
						function = "uart_e";
					};
				};

				e_uart2 {

					mux {
						groups = "uart_e_tx_z11", "uart_e_rx_z12";
						function = "uart_e";
					};
				};

				e_uart3 {

					mux {
						groups = "uart_e_tx_z8", "uart_e_rx_z9";
						function = "uart_e";
					};
				};

				all_nand_pins {

					mux {
						groups = "emmc_nand_d0", "emmc_nand_d1", "emmc_nand_d2", "emmc_nand_d3", "emmc_nand_d4", "emmc_nand_d5", "emmc_nand_d6", "emmc_nand_d7", "nand_ce0", "nand_ale", "nand_cle", "nand_wen_clk", "nand_ren_wr";
						function = "nand";
						input-enable;
					};
				};

				nand_cs {

					mux {
						groups = "nand_ce0";
						function = "nand";
					};
				};

				sd_to_ao_uart_clr_pins {

					mux {
						groups = "GPIOD_0", "GPIOD_1";
						function = "gpio_periphs";
					};
				};

				ao_to_sd_uart_pins {

					mux {
						groups = "uart_b_tx_c", "uart_b_rx_c";
						function = "uart_b";
						bias-pull-up;
						input-enable;
					};
				};

				ao_uart_pins {
					phandle = <0x0f>;

					mux {
						groups = "uart_b_tx_d", "uart_b_rx_d";
						function = "uart_b";
						bias-pull-up;
						input-enable;
					};
				};

				sd_clr_all_pins {

					mux {
						groups = "GPIOC_0", "GPIOC_1", "GPIOC_2", "GPIOC_3", "GPIOC_5";
						function = "gpio_periphs";
						output-high;
					};

					mux1 {
						groups = "GPIOC_4";
						function = "gpio_periphs";
						output-low;
					};
				};

				sd_clr_noall_pins {

					mux {
						groups = "GPIOC_0", "GPIOC_1", "GPIOC_4", "GPIOC_5";
						function = "gpio_periphs";
						output-high;
					};
				};

				sd_1bit_pins {

					mux {
						groups = "sdcard_d0_c", "sdcard_clk_c", "sdcard_cmd_c";
						function = "sdcard";
						bias-pull-up;
						drive-strength-microamp = <0xfa0>;
					};
				};

				sd_iso7816_pins {

					mux {
						groups = "iso7816_clk_h", "iso7816_data_h";
						function = "iso7816";
						input-enable;
						bias-pull-down;
					};
				};

