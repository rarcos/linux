				cpu-passive {
					temperature = <0x14c08>;
					hysteresis = <0x1388>;
					type = "passive";
				};

				cpu-hot {
					temperature = <0x17318>;
					hysteresis = <0x1388>;
					type = "hot";
					phandle = <0x25>;
				};

				cpu-critical {
					temperature = <0x19a28>;
					hysteresis = <0x3e8>;
					type = "critical";
